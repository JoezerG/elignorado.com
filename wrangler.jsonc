{
  "name": "elignorado-com",
  "compatibility_date": "2025-11-04",
  "assets": {
    "directory": "./dist"
  },
  "build": {
    "command": "npm run build -- --mode=production"
  },
  "observability": {
    "enabled": true
  },
  "routes": [
    {
      "pattern": "elignorado.com",
      "custom_domain": true
    },
    {
      "pattern": "www.elignorado.com",
      "custom_domain": true
    }
  ],
  "placement": {
    "mode": "smart"
  }
}